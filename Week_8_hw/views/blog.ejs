<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Blog</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <header>
        <h1>My Blog</h1>
        <nav>
            <a href="/">Home</a>
            <a href="/projects">Projects</a>
            <a href="/blog">Blog</a>
        </nav>
    </header>

    <section>
        <h2>Blog Posts</h2>
        <ul>
            <% blogPosts.forEach(post => { %>
                <li>
                    <h3><%= post.title %></h3>
                    <p><%= post.date %></p>
                    <p><%= post.content %></p>
                </li>
            <% }) %>
        </ul>
    </section>
</body>
</html>
